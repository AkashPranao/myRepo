import java.util.*;

class Main{
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        int num = input.nextInt();
        int stNum = num;
        int max = -1;
        while(num>0){
            int n1 = num%10;
            if(max < n1){
                max = n1;
            }
            num = num/10;

        }

//        a,b,c,d,e,f,g,h,i,j,k,l
        System.out.println(max);
    }
}
